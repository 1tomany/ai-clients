{
    "name": "1tomany/php-ai-clients",
    "description": "",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "openai",
        "open ai",
        "chatgpt",
        "gpt",
        "gemini",
        "claude"
    ],
    "require": {
        "php": ">=8.4",
        "symfony/http-client": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.90",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\AI\\Clients\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OneToMany\\AI\\Clients\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix",
        "stan": "./vendor/bin/phpstan",
        "test": "./vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
